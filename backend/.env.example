# Environment Variables for Backend API
# Copy this file to .env and fill in your actual values

# Qdrant Vector Database (Required)
QDRANT_API_URL=your-qdrant-url
QDRANT_API_KEY=your-qdrant-api-key

# OpenAI (Required for embeddings, optional for LLM)
OPENAI_API_KEY=your-openai-api-key

# GROQ (Optional, but default LLM provider)
GROQ_API_KEY=your-groq-api-key

# LLM Provider Configuration (Optional - defaults to 'groq')
LLM_PROVIDER=groq  # Options: 'groq' or 'openai'
RERANKER_PROVIDER=groq  # Options: 'groq' or 'openai'
